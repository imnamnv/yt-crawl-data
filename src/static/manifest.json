{
  "manifest_version": 3,
  "name": "Mixi Crawl Extension",
  "description": "Mixi Crawl Extension!",
  "version": "1.0.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Extension",
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["alarms"],
  "host_permissions": ["*://*/*"]
}
